
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.scorll_box {
  max-width: 100vw;
  overflow-x: hidden;
  min-height: 100vh;
  overflow-y: auto;
  display: block;
  position: relative;
  top: 0;
  left: 0;
}